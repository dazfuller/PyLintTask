{
    "manifestVersion": 1,
    "id": "pylint-task",
    "name": "PyLint Checker",
    "version": "0.1.1",
    "publisher": "dazfuller",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "images/icon.png"
    },
    "description": "Executes PyLint for checking your Python 3 code base for compliance with PEP-8.",
    "content": {
        "details": {
            "path": "overview.md"
        },
	"license": {
            "path": "LICENSE.md"
        }
    },
    "categories": [
        "Build and release"
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/dazfuller/PyLintTask"
    },
    "files": [
        {
            "path": "pylinttask"
        }
    ],
    "contributions": [
        {
            "id": "pylint-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "pylinttask"
            }
        }
    ]
}